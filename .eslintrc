{
    "extends": "eslint:recommended",
    "env": {
        "browser": true
    },
    "globals": {
        "module": true,
        "require": true,
        "satellite": true,
        "Elm": true,
        "F3": true,
        "F4": true
    },
    "rules": {
        "indent": [
            1,
            2
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "no-trailing-spaces": 1
    }
}
